<h2>Followed Users</h2>
<% if users.exists? %>
 <% @users.each do |f| %>
 <table class="table">
  <thead>
    <tr>
     <th>image</th>
     <th>name</th>
     <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><% image_tag User.all.get_profile_image, size: '50x50' %></td>
      <td><%= f.name %></td>
      <td>フォロー数:<%= f.followings.count %></td>
      <td>フォロワー数:<%= f.followers.count %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  </tbody>
 </table>
  <% end %>
<% else %>
  <p>ユーザーはいません</p>
<% end %>
